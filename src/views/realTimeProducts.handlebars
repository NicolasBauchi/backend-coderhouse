<div class="formulario">
  <h1>Ingresar Producto</h1>
{{!-- action="/realTimeProducts" --}}
  <form  method="POST" id="formularioAgregarProd">
    <input type="text" id="title" name="title" placeholder="ingrese nombre" />
    <input type="text" id="description" name="description" placeholder="ingrese descripcion"/>
    <input type="number" id="price" name="price" placeholder="ingrese Precio" />
    <input type="text" id="thumbnail" name="thumbnail" placeholder="ingrese thumbnail"/>
    <input type="text" id="code" name="code" placeholder="ingrese codigo" />
    <input type="number" id="stock" name="stock" placeholder="ingrese stock" />
    <input type="text" id="category" name="category" placeholder="ingrese categoria" />

    <button type="submit">Agregar producto</button>
  </form>

</div>

<div class="listado">
  <h1>Todos los productos ingresados <b><u>-CON WEBSOCKET-</u></b></h1>
  <ul id="ul_listado">

  </ul>
</div>

<script src="/socket.io/socket.io.js"></script>
<script src="/static/js/websocket.js"></script>

